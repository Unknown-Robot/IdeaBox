{
    "expo": {
        "name": "IdeaBox",
        "slug": "React-native-application",
        "privacy": "public",
        "sdkVersion": "36.0.0",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "package": "com.hetic.ideabox",
            "versionCode": 1
        },
        "ios": {
            "bundleIdentifier": "com.hetic.ideabox",
            "buildNumber": "1.0.0",
            "supportsTablet": true
        }
    }
}